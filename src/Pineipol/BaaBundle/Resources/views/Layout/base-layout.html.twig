<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
        {% block metatags %}{% endblock %}

        <link rel="icon" type="image/png" href="{{ asset('bundles/pineipolbaa/images/favicon.ico') }}">
        <title>{% block title %}Becoming An Agile Architect{% endblock %}</title>

        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/pineipolbaa/css/styles.css') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/pineipolbaa/vendor/css/normalize.css') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/pineipolbaa/vendor/css/component.css') }}" />

        {% endblock %}
        {% block custom_stylesheets %}{% endblock %}

        <script src="{{ asset('bundles/pineipolbaa/vendor/js/modernizr.custom.js') }}"></script>

        {# Analytics code #}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

            ga('create', 'UA-60780686-1', 'auto');
            ga('send', 'pageview');
        </script>
    </head>
    <body>
        {% block outerbody %}{% endblock %}

        <script src="{{ asset('bundles/pineipolbaa/vendor/js/classie.js') }}"></script>
        <script src="{{ asset('bundles/pineipolbaa/vendor/js/sidebarEffects.js') }}"></script>
        {% block custom_javascripts %}{% endblock %}
    </body>
</html>